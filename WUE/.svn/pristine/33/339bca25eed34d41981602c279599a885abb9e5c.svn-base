@charset "UTF-8";
.header {
	position: fixed;
	top: 0;
	width: 100%;
	height: 15%;
	justify-content: center;
	box-sizing: border-box;
	background-color: #DD8EA4;
}

.header__img:first-child {
	position: relative;
}

.header__img img {
	width: 140px;
	height: 140px;
	margin-left: 10px;
	margin-top: -20px;
}

.header__img:last-child {
	position: relative;
	display: flex;
	justify-content: flex-end;
	align-items: center;
	margin-right: 25px;
	margin-top: -130px;
}

.menu {
	position: fixed;
	top: 0;
	width: 20%;
	height: 100%;
	justify-content: flex-start;
	box-sizing: border-box;
}

.menu table {
	position: relative;
	background-color: #FFF0F5;
	margin-top: 113px;
	width: 80%;
	height: 85%;
	font-family: Arial, sans-serif;
}

.login{
	position: relative;
	margin-top: 350px;
	margin-left: 220px;	
}

.to_do_list{
	position: relative;
	margin-top: 200px;
	margin-left: 300px;	
}

.visit{
	position: relative;
	margin-top: 50px;
	margin-left: 300px;	
}

.summary{
	position: relative;
	margin-top: -350px;
	margin-left: 800px;	
}
.update{
   position: fixed;
   top: 200px;
   width: 100%;
   height: 100%;
   align-item:center;
   margin: 150px 0 0 650px;
   font-size: 20px;   
}
.update td input[type="text"]{
   width: 250px;
   height: 20px; 
   font-size: 20px;      
}

.update div input[type="submit"],
.update div input[type="reset"]{
   width: 110px;
   height: 25px; 
   font-size: 15px;      
}
